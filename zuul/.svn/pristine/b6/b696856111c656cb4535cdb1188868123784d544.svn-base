eureka.client.serviceUrl.defaultZone=http://localhost:8088/eureka/
eureka.instance.securePortEnabled=true
spring.application.name=gateway
#server.port=8083
zuul.routes.faq=/service/faq/**
zuul.routes.notification=/service/notification/**

#============================================================================
# Logging Properties 
#============================================================================
logging.level.org.springframework=DEBUG
logging.level.com.capgemini.hrapp=DEBUG
logging.file=../microservices/logs/gateway-service.log


#============================================================================
# SSL Properties 
#============================================================================
server.port: 8449
server.ssl.key-store: classpath:keystore.p12
server.ssl.key-store-password: gateway
server.ssl.keyStoreType: PKCS12
server.ssl.keyAlias: tomcat
